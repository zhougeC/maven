<script>
  (function() {
    $(document).ready(function() {
      var $status, check, checkFailedAttempts, lastLogin, setResult;
  
      $status = $('#statu_captcha').find('i');
      lastLogin = null;
      checkFailedAttempts = function(userLogin) {
        return $.ajax({
          url: '/check_failed_attempts',
          data: {
            userLogin: userLogin
          },
          type: 'POST',
          success: function(data) {
            if (data.result === 1 && data.failed_count > 2) {
              $('.signin-error-captcha').show();
              return $('.wrap-signin-captcha .captcha_img').trigger('click');
            } else {
              $status.removeClass('icon checkmark');
              $status.removeClass('icon remove');
              $('#captcha').val('');
              return $('.signin-error-captcha').hide();
            }
          }
        });
      };
      check = function(val) {
        return $.ajax({
          url: '/check',
          type: 'POST',
          data: {
            'do': 'captcha',
            'val': val
          },
          success: function(data) {
            return setResult(data);
          }
        });
      };
      setResult = function(data) {
        $status.removeClass('icon checkmark');
        $status.removeClass('icon remove');
        if (data === '1') {
          $status.addClass('icon checkmark').css('color', '#6cc644');
          return $('.error-message').html('');
        } else {
          $status.addClass('icon remove').css('color', '#ff2727');
          return $('.error-message').html(data);
        }
      };
      $('.git-login-form-fields .ui.checkbox').checkbox().checkbox({
        onUnchecked: function() {
          return $(this).val(0);
        },
        onChecked: function() {
          return $(this).val(1);
        }
      });
      $('#new_user input').on('keydown', function(e) {
        var captcha, password, username;
  
        if (e.which === 13) {
          username = $('#user_login').val();
          password = $('#user_password').val();
          captcha = $('#captcha').val();
          if (!username) {
            $('#user_login').focus();
            return false;
          }
          if (!password) {
            $('#user_password').focus();
            return false;
          }
          if ($('#captcha').is(':visible')) {
            if ($.trim(captcha).length !== 6) {
              $('#captcha').focus();
              return false;
            }
          }
        }
      });
      $('#user_login').on('blur', function() {
        var userLogin;
  
        userLogin = $('#user_login').val().trim();
        if (lastLogin !== userLogin && userLogin.length > 0) {
          lastLogin = userLogin;
          return checkFailedAttempts(userLogin);
        }
      });
      $('#captcha').bind('input propertychange', function() {
        var captchaVal;
  
        captchaVal = $(this).val().trim();
        if (captchaVal.length === 6) {
          return check(captchaVal);
        } else {
          $status.removeClass('icon checkmark');
          return $('.error-message').html("请输入6位验证码");
        }
      });
      $('.wrap-signin-captcha .captcha_img').click(function() {
        $('#captcha').val('');
        $('.error-message').html('');
        $status.removeClass('icon checkmark');
        return $status.removeClass('icon remove');
      });
      return $('#new_user').on('submit', function() {
        return $('#new_user .submit').addClass('disabled');
      });
    });
  
  }).call(this);
</script>

